{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "keyring-dos",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\keyring\\generate.mjs",
            "outFiles": [
                "${workspaceFolder}/keyring/lib/**/*.js"
            ],
            "cwd": "${workspaceFolder}\\keyring",
            "runtimeArgs": [
                "--experimental-specifier-resolution=node",
                "--preserve-symlinks"
            ]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "keyring-sh",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/keyring/generate.mjs",
            "outFiles": [
                "${workspaceFolder}/keyring/lib/**/*.js"
            ],
            "cwd": "${workspaceFolder}/keyring",
            "runtimeArgs": [
                "--experimental-specifier-resolution=node",
                "--preserve-symlinks"
            ]
        }
    ]
}